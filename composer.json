{
  "name": "lyke/cakephp4-bootstrap-helpers",
  "description": "Bootstrap Helpers for CakePHP 4.0",
  "keywords": [
    "CakePHP",
    "Bootstrap"
  ],
  "license": "MIT",
  "type": "cakephp-plugin",
  "require": {
    "php": ">=7.2.0",
    "cakephp/cakephp": ">=4.0.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~8.4.0",
    "cakephp/cakephp-codesniffer": "^4.0"
  },
  "autoload": {
    "psr-4": {
      "Bootstrap\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Bootstrap\\Test\\": "tests"
    }
  },
  "extra": {
    "installer-name": "Bootstrap"
  }
}
